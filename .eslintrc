{
  "extends": [
    "airbnb-base",
    "plugin:jsx-a11y/recommended",
    "plugin:import/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "assert": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "requireConfigFile": false,
    "babelOptions": {
      "plugins": [
        "@babel/plugin-syntax-import-assertions"
      ]
    }
  },
  "rules": {
    "import/extensions": [0, {
      "css": "never",
      "js": "never",
      "jsx": "never",
      "json": "never"
    }],
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "jsx-a11y/img-has-alt": 0,
    "default-case": 0,
    "func-names": 0,
    "consistent-return": 0,
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "no-console": 0,
    "object-curly-newline": 0,
    "array-bracket-spacing": ["error", "always", {
      "singleValue": false,
      "objectsInArrays": false,
      "arraysInArrays": false
    }],
    "comma-dangle": 0,
    "padded-blocks": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-tabs": 0,
  }
}